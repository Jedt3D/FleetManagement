#tag WindowCode
	#tag Event
		Sub Open()
		  Self.Title = "FleetManagement.Online > Generation Page"
		  ctnMenu1.ctnPageName1.PageName = "Generation Page"
		  ctnPageFooter1.PageFooter = "FleetManagement.Online - Copyright 2019 by Electricity Generating Authority of Thailand"
		  
		  ctnGenThermal1.Visible = True
		  ctnGenPump1.Visible = False
		  ctnGenHydro1.Visible = False
		  ctnGenWind1.Visible = False
		  ctnGenSolar1.Visible = False
		End Sub
	#tag EndEvent

	#tag Event
		Sub Resized()
		  SegmentedControl1.HorizontalCenter = Self.Width/2
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events SegmentedControl1
	#tag Event
		Sub Open()
		  me.HorizontalCenter = Self.Width/2
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Action(SegmentIndex As Integer)
		  Select Case SegmentIndex
		  Case 0
		    ctnGenThermal1.Visible = True
		    ctnGenPump1.Visible = False
		    ctnGenHydro1.Visible = False
		    ctnGenWind1.Visible = False
		    ctnGenSolar1.Visible = False
		  Case 1
		    ctnGenThermal1.Visible = False
		    ctnGenPump1.Visible = True
		    ctnGenHydro1.Visible = False
		    ctnGenWind1.Visible = False
		    ctnGenSolar1.Visible = False
		  Case 2
		    ctnGenThermal1.Visible = False
		    ctnGenPump1.Visible = False
		    ctnGenHydro1.Visible = True
		    ctnGenWind1.Visible = False
		    ctnGenSolar1.Visible = False
		  Case 3
		    ctnGenThermal1.Visible = False
		    ctnGenPump1.Visible = False
		    ctnGenHydro1.Visible = False
		    ctnGenWind1.Visible = True
		    ctnGenSolar1.Visible = False
		  Case 4
		    ctnGenThermal1.Visible = False
		    ctnGenPump1.Visible = False
		    ctnGenHydro1.Visible = False
		    ctnGenWind1.Visible = False
		    ctnGenSolar1.Visible = True
		  End Select
		End Sub
	#tag EndEvent
#tag EndEvents
